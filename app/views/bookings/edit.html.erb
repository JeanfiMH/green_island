<div class ="banner">
  <div class="container">
    <h2>Edit my Booking</h2>
  </div>
</div>

<div class="container">
  <%= simple_form_for ([ current_user, @booking ]) do |f| %>
    <%= f.label :start_date, "Start date" %>
    <%= f.input :start_date %>
    <%= f.label :end_date, "End date" %>
    <%= f.input :end_date %>
    <%= f.submit %>
  <% end %>
</div>
